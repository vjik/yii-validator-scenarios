{
    "name": "vjik/yii-validator-scenarios",
    "type": "library",
    "description": "The scenario feature for Yii Validator",
    "keywords": [
        "yii",
        "validator",
        "validation",
        "scenarios",
        "groups"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/vjik/yii-validator-scenarios/issues?state=open",
        "source": "https://github.com/vjik/yii-validator-scenarios"
    },
    "require": {
        "php": "^8.0",
        "yiisoft/validator": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.25",
        "vimeo/psalm": "^4.30|^5.1"
    },
    "autoload": {
        "psr-4": {
            "Vjik\\Yii\\ValidatorScenarios\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vjik\\Yii\\ValidatorScenarios\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    }
}
